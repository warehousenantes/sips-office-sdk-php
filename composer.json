{
    "name": "worldline/sips-office-sdk",
    "description": "This library simplifies the use of Sips 2.O, the e-payments API.",
    "type": "library",
    "homepage": "https://github.com/nicolasros/sips-office-sdk-php",
    "license": "MIT",
    "keywords": [
        "Sips",
        "Worldline",
        "PSP",
        "Bancontact",
        "Payments"
    ],
    "authors": [
        {
            "name": "Nicolas Ros",
            "email": "supportsips-benelux@worldline.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "php-http/discovery": "^1.0",
        "php-http/httplug": "^2.0",
        "php-http/message-factory": "^1.0",
        "psr/http-client-implementation": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/serializer": "^6.0.7"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.8",
        "php-http/message": "^1.0",
        "php-http/mock-client": "^1.0",
        "rector/rector": "^0.12.20"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Worldline\\Sips\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Worldline\\Sips\\Tests\\": "tests/"
        }
    }
}
